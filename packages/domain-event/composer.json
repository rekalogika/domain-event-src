{
    "name": "rekalogika/domain-event",
    "description": "Domain Event Library",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Priyadi Iman Nurcahyo",
            "email": "priyadi@rekalogika.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Rekalogika\\DomainEvent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rekalogika\\DomainEvent\\Tests\\": "tests/"
        }
    },
    "require": {
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/orm": "^2.16",
        "doctrine/persistence": "^3.2",
        "psr/event-dispatcher": "^1.0",
        "rekalogika/domain-event-contracts": "^1.0",
        "symfony/config": "6.3.*",
        "symfony/event-dispatcher": "6.3.*",
        "symfony/dependency-injection": "6.3.*",
        "symfony/http-kernel": "6.3.*"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^5.15",
        "symfony/uid": "6.3.*"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    }
}
