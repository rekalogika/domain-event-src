{
    "name": "rekalogika/domain-event-src",
    "description": "Domain Event Framework for Symfony",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Priyadi Iman Nurcahyo",
            "email": "priyadi@rekalogika.com"
        }
    ],
    "require": {
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/orm": "^2.16",
        "doctrine/persistence": "^3.2",
        "psr/event-dispatcher": "^1.0",
        "symfony/config": "^6.4 || ^7.0",
        "symfony/dependency-injection": "^6.4 || ^7.0",
        "symfony/event-dispatcher": "^6.4 || ^7.0",
        "symfony/http-kernel": "^6.4 || ^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.5",
        "symfony/framework-bundle": "^6.4 || ^7.0",
        "symfony/stopwatch": "^6.4 || ^7.0",
        "symplify/monorepo-builder": "^11.2",
        "vimeo/psalm": "^5.15"
    },
    "autoload": {
        "psr-4": {
            "Rekalogika\\Contracts\\DomainEvent\\": "packages/domain-event-contracts/src/",
            "Rekalogika\\DomainEvent\\": "packages/domain-event/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rekalogika\\DomainEvent\\Tests\\": "packages/domain-event/tests/"
        }
    },
    "replace": {
        "rekalogika/domain-event": "1.2.3",
        "rekalogika/domain-event-contracts": "1.2.3"
    },
    "minimum-stability": "stable"
}
